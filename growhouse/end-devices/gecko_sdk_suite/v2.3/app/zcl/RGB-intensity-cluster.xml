<?xml version="1.0"?>
<configurator>
  <cluster>
    <name>RGB Intensity Measurement</name>
    <domain>Measurement &amp; Sensing</domain>
    <description>Attributes for reporting light color(R, G, B) intensity.</description>
    <code>0xfd02</code>
    <define>RGB_INTENSITY_MEASUREMENT_CLUSTER</define>
    <client init="false" tick="false">true</client>
    <server init="false" tick="false">true</server>
    <attribute side="server" code="0x0000" define="R_INTENSITY_MEASURED_VALUE" type="INT16U" writable="false" reportable="true" default="0x0000" optional="false">R intensity value</attribute>    <!-- R_INTENSITY_MEASURED_VALUE -->
    <attribute side="server" code="0x0001" define="G_INTENSITY_MEASURED_VALUE" type="INT16U" writable="false" reportable="true" default="0x0000" optional="false">G intensity value</attribute>    <!-- G_INTENSITY_MEASURED_VALUE -->
    <attribute side="server" code="0x0002" define="B_INTENSITY_MEASURED_VALUE" type="INT16U" writable="false" reportable="true" default="0x0000" optional="false">B intensity value</attribute>    <!-- B_INTENSITY_MEASURED_VALUE -->
  </cluster>
</configurator>
